        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="itam.q_puzzle.DBhelper.SettingDBActivity"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="itam.q_puzzle.activity.MenuUtamaActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name="itam.q_puzzle.activity.SetPemainActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name="itam.q_puzzle.activity.LoadingScreenActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name="itam.q_puzzle.activity.PuzzleBoardActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name="itam.q_puzzle.activity.NilaiPermainanActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name="itam.q_puzzle.activity.PapanNilaiActivity"
            android:screenOrientation="landscape" />
